#!/usr/bin/env bash

if [ ! -z "${DEV_BASE// }" ]
then
  cd $DEV_BASE/py/xlattice_py
  
  rm -rf build/* myData/* myU1/* myU2/* tmp/* 
  
  pySloc -L py scripts/gen_node_id1 scripts/gen_node_id3 u_consolidate u_preen u_re_struc u_stats verify_content_keys  -X cover -X htmlcov -X dist -v $@
else
  echo "DEV_BASE is not defined"
fi
